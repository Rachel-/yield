<div class="page-header">
	<%= link_to l(:label_custom_field_new), new_custom_field_path, :class => 'btn btn-default pull-right' %>
	<h2><%= l(:label_custom_field_plural) %></h2>
</div>

<% if @custom_fields_by_type.present? %>
  <%= render_custom_fields_tabs(@custom_fields_by_type.keys) %>
<% else %>
  <div class="nodata alert alert-warning"><%= l(:label_no_data) %></div>
<% end %>
